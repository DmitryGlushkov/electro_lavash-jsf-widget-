<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">
    <namespace>http://electrolavash.ru/jsf/html</namespace>

    <tag>
        <tag-name>blur</tag-name>
        <handler-class>ru.electrolavash.blur.BlurHandler</handler-class>
        <attribute>
            <name>class</name><required>true</required><type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>disabled</name><required>false</required><type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>delay</name><required>false</required><type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>resetValues</name><required>false</required><type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>event</name><required>false</required><type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>execute</name><required>false</required><type>java.util.Collection</type>
        </attribute>
        <attribute>
            <name>immediate</name><required>false</required><type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>listener</name><required>false</required><method-signature>public voidprocessAjaxBehavior(javax.faces.event.AjaxBehaviorEvent event)throws javax.faces.event.AbortProcessingException</method-signature>
        </attribute>
        <attribute>
            <name>onerror</name><required>false</required><type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>render</name><required>false</required><type>java.util.Collection</type>
        </attribute>
    </tag>

</facelet-taglib>